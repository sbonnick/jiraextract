{
  "name": "jiraextract",
  "version": "1.0.0",
  "engines": {
    "node": ">=12.4.0"
  },
  "description": "extracts data via jira api and loads into a database",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "cross-env LOG_ENABLED=false jest",
    "debugtest": "cross-env LOG_ENABLED=true jest",
    "coverage": "cross-env LOG_ENABLED=false jest --reporters=text-lcov > coverage.lcov && codecov",
    "npmcheck": "node node_modules/npm-check/bin/cli.js"
  },
  "author": "stewart bonnick",
  "license": "MIT",
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageDirectory": "coverage",
    "verbose": false
  },
  "dependencies": {
    "@atlassian/jira": "^0.1.0",
    "@lifeomic/attempt": "^3.0.0",
    "@types/jest": "^24.0.17",
    "humanize-duration": "^3.20.1",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "moment-weekday-calc": "^1.1.3",
    "node-schedule": "^1.3.1",
    "pg": "^7.12.0",
    "pino": "^5.13.1",
    "pino-pretty": "^3.2.0"
  },
  "devDependencies": {
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-editorconfig": "^2.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "glob": "^7.1.4",
    "jest": "^24.8.0",
    "npm-check": "^5.9.0",
    "nyc": "^14.1.1"
  }
}

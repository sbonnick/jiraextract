{
  "name": "jiraextract",
  "version": "1.0.0",
  "engines": {
    "node": ">=12.4.0"
  },
  "description": "extracts data via jira api and loads into a database",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "cross-env LOG_ENABLED=false jest",
    "debugtest": "cross-env LOG_ENABLED=true jest",
    "coverage": "cross-env LOG_ENABLED=false jest --reporters=text-lcov > coverage.lcov && codecov",
    "npmcheck": "node node_modules/npm-check/bin/cli.js",
    "lint": "eslint .",
    "watch": "jest --watch"
  },
  "author": "stewart bonnick",
  "license": "MIT",
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageDirectory": "coverage",
    "verbose": false,
    "watchPlugins": [
      "jest-runner-eslint/watch-fix"
    ],
    "projects": [
      {
        "displayName": "unit tests"
      },
      {
        "runner": "jest-runner-eslint",
        "displayName": "lint",
        "testMatch": [
          "<rootDir>/src/**/*.js",
          "<rootDir>/test/**/*.js"
        ]
      }
    ]
  },
  "dependencies": {
    "@atlassian/jira": "^0.1.0",
    "@lifeomic/attempt": "^3.0.0",
    "@types/jest": "^24.0.18",
    "humanize-duration": "^3.21.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "moment-weekday-calc": "^1.1.3",
    "node-schedule": "^1.3.1",
    "pg": "^7.12.1",
    "pino": "^5.13.4",
    "pino-pretty": "^3.2.2",
    "sequelize": "^5.19.6"
  },
  "devDependencies": {
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-editorconfig": "^2.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "jest-runner-eslint": "^0.7.5",
    "npm-check": "^5.9.0",
    "sequelize-mock": "^0.10.2"
  }
}

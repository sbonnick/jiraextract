{
  "name": "jiraextract",
  "version": "1.0.0",
  "engines": {
    "node": ">=12.4.0"
  },
  "description": "extracts data via jira api and loads into a database",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "cross-env LOG_ENABLED=false nyc mocha --recursive",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "author": "stewart bonnick",
  "license": "MIT",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "@atlassian/jira": "^0.1.0",
    "debug": "^4.1.1",
    "humanize-duration": "^3.17.0",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "moment-weekday-calc": "^1.1.3",
    "node-schedule": "^1.3.1",
    "pg": "^7.11.0",
    "pino": "^5.12.6",
    "pino-pretty": "^3.1.0",
    "promise-retry": "^1.1.1",
    "promise.allsettled": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-editorconfig": "^1.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "testdouble": "^3.11.0",
    "tmp-promise": "^2.0.1"
  }
}
